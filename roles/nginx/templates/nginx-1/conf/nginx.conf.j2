events{}
http {
        server {
        listen {{ nginx_1_port_2}} default_server;
        listen [::]:{{ nginx_1_port_2}} default_server;

        root /var/www/html;

        index index.html;

        server_name localhost;

        location / {
                try_files $uri $uri/ =404;
        }

        location /metrics {
                stub_status on;
        }
}
}